use Utopian\Scheduler;
use Utopian\Socket\HttpConnect;

require_once 'vendor/autoload.php';

$scheduler = new Scheduler();
$scheduler->monitor(new HttpConnect(),'0.0.0.0:8080');
$scheduler->run();